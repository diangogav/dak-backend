{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "strictPropertyInitialization": false,
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "Bearer": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "outputDirectory": "./src",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "src/routes"
  }
}